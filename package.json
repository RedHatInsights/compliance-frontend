{
  "name": "compliance-frontend",
  "version": "1.1.0",
  "private": false,
  "insights": {
    "appname": "compliance"
  },
  "dependencies": {
    "@apollo/client": "^3.9.10",
    "@babel/runtime": "^7.24.0",
    "@patternfly/patternfly": "^5.0.2",
    "@patternfly/react-charts": "^7.2.2",
    "@patternfly/react-core": "^5.2.1",
    "@patternfly/react-icons": "^5.2.1",
    "@patternfly/react-table": "^5.2.1",
    "@patternfly/react-tokens": "^5.2.1",
    "@react-pdf/renderer": "^1.6.17",
    "@redhat-cloud-services/frontend-components": "^4.2.7",
    "@redhat-cloud-services/frontend-components-notifications": "^4.1.0",
    "@redhat-cloud-services/frontend-components-pdf-generator": "^4.0.0",
    "@redhat-cloud-services/frontend-components-remediations": "^3.2.12",
    "@redhat-cloud-services/frontend-components-utilities": "^4.0.6",
    "axios": "^1.6.8",
    "graphql": "^16.8.1",
    "json-api-normalizer": "1.0.1",
    "linkifyjs": "^2.1.9",
    "p-all": "^5.0.0",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.8.1",
    "redux": "^4.2.1",
    "redux-form": "^8.3.10",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2"
  },
  "overrides": {
    "@react-pdf/renderer": {
      "react": "^18.2.0"
    }
  },
  "sassIncludes": {
    "patternfly": "node_modules/patternfly/dist/sass",
    "bootstrap": "node_modules/patternfly/node_modules/bootstrap-sass/assets/stylesheets",
    "fontAwesome": "node_modules/patternfly/node_modules/font-awesome-sass/assets/stylesheets"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "coverageDirectory": "./jest-coverage/",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.cy.js",
      "!src/**/stories/*",
      "!src/{App,Routes,entry,entry-dev}.js",
      "!src/**/__fixtures__/*.js",
      "!src/**/__factories__/*.js",
      "!src/{SmartComponents,PresentationalComponents,Mutations,store}/index.js",
      "!src/Utilities/{asyncComponent,ReducerRegistry,Dispatcher}.js"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(uuid|p-all|p-map|aggregate-error|indent-string|clean-stack))"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|svg)$": "identity-obj-proxy",
      "^SmartComponents$": "<rootDir>/src/SmartComponents",
      "^SmartComponents/(.*)": "<rootDir>/src/SmartComponents/$1",
      "^PresentationalComponents$": "<rootDir>/src/PresentationalComponents",
      "^PresentationalComponents/(.*)": "<rootDir>/src/PresentationalComponents/$1",
      "^Mutations(|/.*)": "<rootDir>/src/Mutations$1",
      "^Utilities/(.*)": "<rootDir>/src/Utilities/$1",
      "^Store/(.*)": "<rootDir>/src/store/$1",
      "^Store$": "<rootDir>/src/store",
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "nyc": {
    "report-dir": "cypress-coverage",
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "src/**/*.test.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/travis-cli": "^19.3.0",
    "@cypress/code-coverage": "^3.12.29",
    "@cypress/react18": "^2.0.0",
    "@cypress/webpack-dev-server": "^3.7.4",
    "@faker-js/faker": "^8.4.1",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^2.0.3",
    "@redhat-cloud-services/frontend-components-config": "^6.0.16",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.10",
    "@testing-library/jest-dom": "^6.4.3",
    "@testing-library/react": "^15.0.6",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.58",
    "babel-loader": "^9.1.2",
    "babel-plugin-istanbul": "^6.1.1",
    "css-loader": "^6.8.1",
    "cypress": "^13.7.0",
    "dotenv-defaults": "^5.0.2",
    "eslint": "^8.43.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "fishery": "^2.2.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "natsort": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.64.1",
    "stylelint": "^15.10.2",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-scss": "^5.0.1",
    "ts-patch": "^3.1.2",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "fec build",
    "test": "jest --passWithNoTests",
    "test:nocoverage": "jest --passWithNoTests --no-coverage",
    "test:snapshots": "jest -u --passWithNoTests --no-coverage",
    "test:ct": "BABEL_ENV=componentTest cypress run --component",
    "test:openct": "cypress open --component",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "prod": "NODE_ENV=production webpack serve --config config/prod.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "fec dev",
    "start:proxy": "PROXY=true fec dev",
    "travis:verify": "npm-run-all build lint test && npm run test:ct -- --browser chrome",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run travis:verify",
    "bump:redhatservices": "npm install @redhat-cloud-services/frontend-components-notifications@latest @redhat-cloud-services/frontend-components-utilities@latest @redhat-cloud-services/frontend-components-config@latest @redhat-cloud-services/frontend-components@latest @redhat-cloud-services/frontend-components-remediations@latest @redhat-cloud-services/eslint-config-redhat-cloud-services@latest",
    "bump:patternfly": "npm install @patternfly/react-core@latest @patternfly/react-table@latest @patternfly/react-charts@latest @patternfly/react-tokens@latest @patternfly/react-icons@latest",
    "coverage:clean": "rm -rf .nyc_output coverage reports",
    "coverage": "bash coverage.sh && npm run coverage:clean",
    "postinstall": "ts-patch install"
  }
}
