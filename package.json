{
  "name": "compliance-frontend",
  "version": "1.1.0",
  "private": false,
  "insights": {
    "appname": "compliance"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@data-driven-forms/pf4-component-mapper": "^4.1.7",
    "@data-driven-forms/react-form-renderer": "^4.1.6",
    "@patternfly/patternfly": "^6.3.1",
    "@patternfly/react-charts": "^8.3.1",
    "@patternfly/react-component-groups": "^6.3.0",
    "@patternfly/react-core": "^6.3.1",
    "@patternfly/react-icons": "^6.3.1",
    "@patternfly/react-table": "^6.3.1",
    "@patternfly/react-tokens": "^6.3.1",
    "@redhat-cloud-services/compliance-client": "^3.0.1",
    "@redhat-cloud-services/frontend-components": "^7.0.5",
    "@redhat-cloud-services/frontend-components-config-utilities": "^4.6.0",
    "@redhat-cloud-services/frontend-components-notifications": "^6.1.7",
    "@redhat-cloud-services/frontend-components-remediations": "^4.0.18",
    "@redhat-cloud-services/frontend-components-utilities": "^7.0.5",
    "@redhat-cloud-services/javascript-clients-shared": "^2.0.0",
    "@sentry/webpack-plugin": "^2.23.1",
    "@tanstack/react-query": "^5.90.5",
    "@unleash/proxy-client-react": "^3.6.0",
    "axios": "^1.12.2",
    "bastilian-tabletools": "^2.18.1",
    "linkify-html": "^4.3.2",
    "linkifyjs": "^4.3.2",
    "p-all": "^5.0.0",
    "p-map": "^7.0.3",
    "react": "^18.3.1",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.3.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.26.1",
    "redux": "^4.2.1",
    "redux-form": "^8.3.10",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.2.0",
    "semver": "^7.7.2",
    "use-deep-compare": "^1.3.0",
    "use-deep-compare-effect": "^1.8.1",
    "victory": "^37.3.6"
  },
  "overrides": {
    "react-redux": "^8.1.3",
    "better-docs": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    },
    "vue-docgen-api": {
      "pug": "^3.0.1"
    }
  },
  "nyc": {
    "report-dir": "coverage-cypress",
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "src/**/*.test.js",
      "cypress/**/*.js",
      "config/**/*.js",
      "src/**/*.test.js",
      "src/**/*.cy.js",
      "src/{App,entry,entry-dev}.js",
      "**/__fixtures__/**",
      "**/__factories__/**"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cypress/code-coverage": "^3.14.7",
    "@cypress/react18": "^2.0.1",
    "@faker-js/faker": "^9.9.0",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^3.0.0",
    "@redhat-cloud-services/frontend-components-config": "^6.6.9",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.25",
    "@tanstack/eslint-plugin-query": "^5.86.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.23",
    "babel-loader": "^10.0.0",
    "babel-plugin-istanbul": "^7.0.1",
    "better-docs": "^2.7.3",
    "css-loader": "^7.1.2",
    "cypress": "^15.4.0",
    "dotenv-defaults": "^5.0.2",
    "eslint": "^9.33.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsdoc": "^54.3.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.6.8",
    "fishery": "^2.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jsdoc": "^4.0.4",
    "natsort": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "react-intl": "^7.1.11",
    "redux-mock-store": "^1.5.5",
    "sass": "^1.92.0",
    "stylelint": "^16.23.1",
    "stylelint-config-recommended-scss": "^16.0.0",
    "stylelint-scss": "^6.12.1",
    "ts-patch": "^3.3.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  },
  "scripts": {
    "build": "fec build",
    "test": "jest --passWithNoTests",
    "test:nocoverage": "jest --passWithNoTests --no-coverage",
    "test:snapshots": "jest -u --passWithNoTests --no-coverage",
    "test:ct": "BABEL_ENV=componentTest cypress run --component",
    "test:openct": "BABEL_ENV=componentTest cypress open --component",
    "test:coverage": "curl -sSL 'https://raw.githubusercontent.com/RedHatInsights/insights-interact-tools/refs/heads/main/scripts/coverage.sh' | bash",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "prod": "NODE_ENV=production webpack serve --config config/prod.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "fec dev",
    "static": "fec static",
    "start:proxy": "PROXY=true fec dev",
    "verify": "npm-run-all build lint test",
    "ci:verify": "npm run test:coverage",
    "nightly": "npm run ci:verify",
    "bump:redhatservices": "npm update @redhat-cloud-services/compliance-client @redhat-cloud-services/frontend-components-notifications @redhat-cloud-services/frontend-components-utilities @redhat-cloud-services/frontend-components-config @redhat-cloud-services/frontend-components @redhat-cloud-services/frontend-components-remediations @redhat-cloud-services/eslint-config-redhat-cloud-services @redhat-cloud-services/tsc-transform-imports @redhat-cloud-services/frontend-components-config-utilities --save",
    "bump:patternfly": "npm update @patternfly/react-core @patternfly/react-table @patternfly/react-charts @patternfly/react-tokens @patternfly/react-icons --save",
    "postinstall": "ts-patch install",
    "docs": "jsdoc --configure .jsdoc.json"
  }
}
